package vererbung1;

public class Animal {

	// Attribute
	String name;
	int age;
	private String color;
	String favFood;

	// Methoden

	public void sleep() {
		System.out.println("Ich schlafe gerade...");
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}
	
	

}
